<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signUp</servlet-name>
        <servlet-class>com.tanda.servlet.signUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logIn</servlet-name>
        <servlet-class>com.tanda.servlet.logIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signOut</servlet-name>
        <servlet-class>com.tanda.servlet.signOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prueba</servlet-name>
        <servlet-class>prueba</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfo</servlet-name>
        <servlet-class>com.tanda.servlet.UserInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserInfo</servlet-name>
        <servlet-class>com.tanda.servlet.EditUserInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewTandas</servlet-name>
        <servlet-class>com.tanda.servlet.ViewTandas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewPagos</servlet-name>
        <servlet-class>com.tanda.servlet.ViewPagos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewTanda</servlet-name>
        <servlet-class>com.tanda.servlet.NewTanda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveTanda</servlet-name>
        <servlet-class>com.tanda.servlet.RemoveTanda</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUp</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logIn</servlet-name>
        <url-pattern>/logIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signOut</servlet-name>
        <url-pattern>/signOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prueba</servlet-name>
        <url-pattern>/prueba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInfo</servlet-name>
        <url-pattern>/view/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserInfo</servlet-name>
        <url-pattern>/edit/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewTandas</servlet-name>
        <url-pattern>/view/tandas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewPagos</servlet-name>
        <url-pattern>/view/pagos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewTanda</servlet-name>
        <url-pattern>/new/tanda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveTanda</servlet-name>
        <url-pattern>/remove/tanda</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
